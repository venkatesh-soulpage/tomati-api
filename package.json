{
  "name": "boozeboss-api",
  "version": "0.16.2",
  "private": true,
  "scripts": {
    "start": "nodemon -r esm ./bin/www",
    "start:dev": "env-cmd -f .env nodemon -r esm ./bin/www",
    "start:prod": "env-cmd -f .env.prod nodemon -r esm ./bin/www",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "populate_organizations": "knex seed:run --specific=populate_organizations_data.js",
    "reset_organizations": "knex seed:run --specific=reset_organizations_data.js",
    "populate_briefs": "knex seed:run --specific=populate_brief_data.js",
    "reset_briefs": "knex seed:run --specific=reset_brief_data.js",
    "populate_products": "knex seed:run --specific=populate_products_data.js",
    "reset_products": "knex seed:run --specific=reset_products_data.js",
    "populate_warehouses": "knex seed:run --specific=populate_inventory_data.js",
    "reset_warehouses": "knex seed:run --specific=reset_inventory_data.js",
    "populate_requisitions": "knex seed:run --specific=populate_requisitions_data.js",
    "reset_requisitions": "knex seed:run --specific=reset_requisitions_data.js",
    "populate_events": "knex seed:run --specific=populate_events_data.js",
    "reset_events": "knex seed:run --specific=reset_events_data.js",
    "populate_wallet": "knex seed:run --specific=populate_wallet_data.js",
    "reset_wallet": "knex seed:run --specific=reset_wallet_data.js",
    "populate_countries": "knex seed:run --specific=initial_locations.js",
    "populate_roles": "knex seed:run --specific=roles.js",
    "populate_all": "npm run populate_organizations && npm run populate_products && npm run populate_warehouses &&  npm run populate_briefs && npm run populate_requisitions && npm run populate_events && npm run populate_wallet",
    "reset_all": "npm run reset_wallet && npm run reset_events && npm run reset_requisitions && npm run reset_briefs && npm run reset_warehouses && npm run reset_products && npm run reset_organizations"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@octokit/auth-app": "^2.4.2",
    "aws-sdk": "^2.667.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dynamic-qr-code-generator": "^0.1.0",
    "email-templates": "^6.0.3",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-validator": "^6.4.0",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "hellosign-sdk": "^1.4.1",
    "http-errors": "^1.6.3",
    "is-base64": "^1.1.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.2.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.20",
    "moment": "^2.25.3",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "objection": "^2.1.3",
    "pdfkit": "^0.11.0",
    "pg": "^8.0.0",
    "postgres-date": "^1.0.5",
    "pug": "^2.0.0-beta11",
    "qr-image": "^3.2.0",
    "qrcode": "1.x",
    "qrcode-with-logos": "^1.0.3",
    "query-string": "^6.10.1",
    "react-qr-code": "^1.0.3",
    "twilio": "^3.43.1"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1"
  }
}
